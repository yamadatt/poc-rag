{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npx vite",
    "build": "npx tsc -b && npx vite build",
    "build:fast": "npx vite build",
    "build:staging": "npx tsc -b && npx vite build --mode staging",
    "build:production": "npx tsc -b && npx vite build --mode production",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint . --fix",
    "typecheck": "npx tsc --noEmit",
    "format": "npx prettier --write .",
    "format:check": "npx prettier --check .",
    "preview": "npx vite preview",
    "test": "npx jest",
    "test:unit": "npx jest --selectProjects unit",
    "test:integration": "npx jest --selectProjects integration",
    "test:watch": "npx jest --watch",
    "test:coverage": "npx jest --coverage",
    "test:e2e": "npx playwright test",
    "test:e2e:ui": "npx playwright test --ui",
    "test:e2e:headed": "npx playwright test --headed",
    "test:e2e:debug": "npx playwright test --debug",
    "validate": "npm run typecheck && npm run lint && npm run test"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.7",
    "@heroicons/react": "^2.2.0",
    "@reduxjs/toolkit": "^2.8.2",
    "@tailwindcss/postcss": "^4.1.12",
    "axios": "^1.11.0",
    "chart.js": "^4.5.0",
    "react": "^19.1.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.1",
    "react-dropzone": "^14.3.8",
    "react-redux": "^9.2.0",
    "react-router-dom": "^6.26.2"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@eslint/js": "^9.33.0",
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "msw": "^2.10.5",
    "playwright": "^1.55.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.12",
    "ts-jest": "^29.4.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^5.4.10"
  }
}
